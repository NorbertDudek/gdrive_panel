{
  "browser_specific_settings": {
    "gecko": {
      "id": "gdrive_panel@norbertdudek.com.ar"
    }
  },
  "manifest_version": 2,
  "name": "Pinned GDrive",
  "version": "1.2.0",
  "description": "Go to the GDrive tab. If it doesn't exist, it's created.",
  "icons": {
    "48": "icons/gdrive-48.png",
    "64": "icons/gdrive-64.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icons/gdrive-32.png"
    },
    "default_title": "Pinned GDrive",
    "default_area": "navbar"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}